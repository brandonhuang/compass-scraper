<div class="container">
  <h1>Compass Assignments</h1>
  <form class="" action="/" method="GET">
    <input type="text" name="query" placeholder="Keyword" required>
    <input type="submit" value="Search">
  </form>
  <% @results.each do |result| %>
      <div>
        <a href=<%="http://compass.lighthouselabs.ca/days/setup/activities/#{result.id}" %>><%= "#{result.id} - #{result.title}" %></a>
      </div>
  <% end %>
</div>
